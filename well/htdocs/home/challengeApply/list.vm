<a  class="btn btn-default"  href="$!homeServer.getURI("challengeApply/create.htm")" >新建</a>

<table  class="table">
    <thead>
 <tr>
              <th>约战ID</th>
              <th>球队ID</th>
              <th>比赛进球数</th>
              <th>比赛结果</th>
              <th>是否接受</th>
          <th>操作</th>
 </tr>
    </thead>
#foreach($f in $challengeApplyDOList)
  <tr>
               <td> $!f.challengeId</td>
               <td> $!f.teamId</td>

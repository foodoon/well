<form class="form-inline" role="form">
    <fieldset>
        <div id="legend" class="">
            <legend class="">服务信息</legend>
        </div>
        <div class="form-group">
            <label>服务端地址</label>
            <input type="text" class="form-control" value="http://blog.foodoon.com/api.htm">
        </div>

        <div class="form-group">
            <label>apiName</label>
            <input type="text" class="form-control" value="$apiDefine.apiName">
        </div>
        <div class="form-group">
            <label>apiVersion</label>
            <input type="text" class="form-control" value="$apiDefine.apiVersion">
        </div>
    </fieldset>
</form>

<form class="form-inline" role="form">

    <fieldset>
        <div id="legend" class="">
            <legend class="">参数信息</legend>
        </div>
        #foreach($apiParam in $apiDefine.params.entrySet())
            <div class="form-group">
                <label>$apiParam.value</label>
                <input type="text" class="form-control" name="$apiParam.key" placeholder="$apiParam.key">
            </div>
        #end

    </fieldset>

</form>

